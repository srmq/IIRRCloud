{
    "name": "srmq/iirr-cloud",
    "description": "Cloud Service for the Intelligent Irrigator (IIRR)",
    "type": "project",
    "require": {
        "monolog/monolog": "^1.24",
        "slim/slim": "^3.12",
        "relay/relay": "2.x@dev",
        "middlewares/http-authentication": "^1.1",
